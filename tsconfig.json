{
	"compilerOptions": {
		"target": "ESNext",
		"jsx": "react-jsx",
		"module": "ESNext",
		"moduleResolution": "node",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"types": ["react", "react-dom"]
	},
	"include": ["src", "lib"],
	"exclude": ["**/node_modules", "**/dist", "**/.turbo"]
}
