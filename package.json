{
	"name": "secret-lib",
	"version": "0.2.3",
	"license": "MIT",
	"author": "Matheus Boeira <matheusb_dias@hotmail.com>",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/matheusboeira/secret-lib.git"
	},
	"publishConfig": {
		"access": "public"
	},
	"keywords": ["react", "hooks", "components", "tailwindcss"],
	"main": "dist/index.cjs",
	"types": "dist/index.d.ts",
	"files": ["dist", "README.md", "LICENSE"],
	"sideEffects": false,
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/lib/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		},
		"./hooks": {
			"types": "./dist/lib/hooks/index.d.ts",
			"import": "./dist/lib/hooks/index.mjs",
			"require": "./dist/lib/hooks/index.cjs"
		},
		"./tooltip": {
			"types": "./dist/lib/components/tooltip/index.d.ts",
			"import": "./dist/lib/components/tooltip/index.mjs",
			"require": "./dist/lib/components/tooltip/index.cjs"
		},
		"./autocomplete": {
			"types": "./dist/lib/components/autocomplete/index.d.ts",
			"import": "./dist/lib/components/autocomplete/index.mjs",
			"require": "./dist/lib/components/autocomplete/index.cjs"
		}
	},
	"scripts": {
		"build": "vite build --mode lib && tsc --project tsconfig.lib.json",
		"build:preview": "vite build --mode app && tsc --project tsconfig.build.json",
		"prepublish": "npm run build",
		"publish:major": "npm version major && npm publish --access public",
		"publish:minor": "npm version minor && npm publish --access public",
		"publish:patch": "npm version patch && npm publish --access public",
		"dev": "vite",
		"preview": "vite preview"
	},
	"peerDependencies": {
		"clsx": "^2.0.0",
		"react": "^18.0.0",
		"react-dom": "^18.0.0",
		"tailwind-merge": "^3.2.0",
		"tailwindcss": "^3.4.17"
	},
	"dependencies": {
		"virtua": "^0.40.4"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/lodash": "^4.17.16",
		"@types/react": "^19.1.2",
		"@types/react-dom": "^19.1.2",
		"@vitejs/plugin-react": "^4.4.1",
		"autoprefixer": "^10.0.0",
		"postcss": "^8.0.0",
		"react-scan": "^0.3.3",
		"rollup-plugin-visualizer": "^5.14.0",
		"typescript": "^5.0.0",
		"vite": "^6.3.2",
		"vite-tsconfig-paths": "^5.1.4",
		"zustand": "^5.0.3"
	}
}
