{
	"name": "secret-lib",
	"version": "0.1.10",
	"license": "MIT",
	"author": "Matheus Boeira",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matheusboeira/secret-lib.git"
  },
	"keywords": [
		"react",
		"hooks",
		"components",
		"tailwindcss"
	],
	"main": "dist/index.cjs",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"files": ["dist"],
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		}
	},
	"sideEffects": false,
	"scripts": {
		"build": "tsup lib/index.ts --dts --format esm,cjs --out-dir dist",
		"prepublish": "npm run build",
		"publish:major": "npm version major && npm publish --access public",
		"publish:minor": "npm version minor && npm publish --access public",
		"publish:patch": "npm version patch && npm publish --access public",
		"analyze": "pnpm build && source-map-explorer dist/index.js dist/index.js.map",
		"analyze:esm": "pnpm build && source-map-explorer dist/index.mjs dist/index.mjs.map",
		"gzip-size": "pnpm build && gzip-size dist/index.js --raw",
		"dev": "vite"
	},
	"peerDependencies": {
		"react": "^18.0.0",
		"react-dom": "^18.0.0",
		"tailwindcss": "^3.4.17"
	},
	"dependencies": {
		"clsx": "^2.0.0",
		"tailwind-merge": "^3.2.0"
	},
	"devDependencies": {
		"lodash": "^4.17.21",
		"@types/lodash": "^4.17.16",
		"@types/react": "^19.1.0",
		"@types/react-dom": "^19.1.2",
		"@vitejs/plugin-react": "^4.0.0",
		"autoprefixer": "^10.0.0",
		"gzip-size-cli": "^5.1.0",
		"postcss": "^8.0.0",
		"react-scan": "^0.3.3",
		"source-map-explorer": "^2.5.3",
		"tsup": "^8.4.0",
		"typescript": "^5.0.0",
		"vite": "^6.2.6",
		"vite-tsconfig-paths": "^5.1.4"
	}
}
